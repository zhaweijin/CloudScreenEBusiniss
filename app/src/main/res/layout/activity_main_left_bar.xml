<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:background="@drawable/home_left_bar_bg"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">


    <RelativeLayout
        android:id="@+id/layout_domy_icon"
        android:layout_width="match_parent"
        android:layout_height="@dimen/length_160"
        android:orientation="horizontal">

        <LinearLayout
            android:orientation="vertical"
            android:layout_centerInParent="true"
            android:background="@drawable/domybox_shop_icon"
            android:layout_width="@dimen/width_mainactivity_71"
            android:layout_height="@dimen/height_mainactivity_68">

        </LinearLayout>
        <ImageView
            android:background="@drawable/home_left_horizontal"
            android:layout_alignParentBottom="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/layout_domy_user_center"
        android:layout_width="match_parent"
        android:layout_height="@dimen/length_120"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:nextFocusDown="@+id/layout_domy_shopping_cart"
        android:orientation="horizontal">

        <LinearLayout
            android:orientation="vertical"
            android:layout_centerInParent="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/domy_model_icon"
                android:layout_width="@dimen/gap"
                android:layout_height="@dimen/gap"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/domybox"
                />
            <TextView
                android:id="@+id/model"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginTop="@dimen/length_15"
                android:textSize="@dimen/sp_16"
                android:textColor="@android:color/white"
                android:text="我的" />
        </LinearLayout>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/layout_domy_shopping_cart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/home_shopping_cart_bg"
        android:focusable="true"
        android:focusableInTouchMode="true"
        android:orientation="horizontal">


    </RelativeLayout>

</LinearLayout>